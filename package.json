{
  "name": "custom-local-domain",
  "version": "1.0.0",
  "description": "Edits the /etc/hosts file on a machine to redirect local domains to a proxy which forwards to specific ports",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "nodemon --watch \".\\index.ts\" --exec \"ts-node\" .\\index.ts",
    "build": "tsc",
    "start": "ts-node index.ts",
    "prepublish": "trash dist/config.json"
  },
  "keywords": [],
  "author": "Tom Bonner",
  "license": "MIT",
  "bin": {
    "custom-local-domain": "./dist/index.js"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "debounce": "^1.2.0",
    "express": "^4.17.1",
    "hostile": "^1.3.2",
    "http-proxy": "^1.18.1",
    "is-elevated": "^3.0.0",
    "uri-js": "^4.2.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "trash-cli": "^3.0.0",
    "nodemon": "^2.0.4",
    "@types/express": "^4.17.7",
    "@types/http-proxy": "^1.17.4",
    "ts-node-dev": "^1.0.0-pre.52",
    "typescript": "^3.9.7"
  }
}
